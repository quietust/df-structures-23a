                <stl-vector name='leather2' type-name='int16_t'/>
                <stl-vector name='metal' type-name='int16_t'/>
                <stl-vector name='booze' type-name='int16_t'/>
                <stl-vector name='cheese' type-name='int16_t'/>
                <stl-vector name='powders' type-name='int16_t'/>
                <stl-vector name='extracts' type-name='int16_t'/>
                <stl-vector name='meat' type-name='int16_t'/>
            </compound>

            <stl-vector name='fish_races'>
                <int32_t ref-target='creature_raw'/>
            </stl-vector>

            <stl-vector name='plants' type-name='int16_t'/>
            <stl-vector name='seeds' type-name='int16_t'/>

            <compound name='wood_products' comment='lye, charcoal, potash, pearlash, and coke'>
                <stl-vector name='item_type'>
                    <enum base-type='int16_t' type-name='item_type'/>
                </stl-vector>
                <stl-vector type-name='int16_t' name='item_subtype'/>
                <stl-vector name='material' type-name='int16_t'/>
            </compound>

            <compound name='animals'>
                <stl-vector name='pet_races'>
