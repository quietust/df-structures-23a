<data-definition>
    <struct-type type-name='descriptor_color' instance-vector='$global.world.raws.descriptors.colors'>
        <stl-string name="id"/>
        <code-helper name='describe'>$.id</code-helper>
        <stl-vector name="words_str">
            <pointer type-name='stl-string'/>
        </stl-vector>
        <stl-vector name="words">
            <int32_t ref-target='language_word'/>
        </stl-vector>
        <stl-string name="name"/>
        <int8_t name="fore"/>
        <int8_t name="bright"/>
        <s-float name="red"/>
        <s-float name="green"/>
        <s-float name="blue"/>
    </struct-type>

    <struct-type type-name='descriptor_shape' instance-vector='$global.world.raws.descriptors.shapes'>
        <stl-string name="id"/>
        <code-helper name='describe'>$.id</code-helper>
        <stl-vector name="words_str">
            <pointer type-name='stl-string'/>
        </stl-vector>
        <stl-vector name="words">
            <int32_t ref-target='language_word'/>
        </stl-vector>
        <stl-string name="name"/>
        <stl-string name="name_plural"/>
        <stl-vector name="adj">
            <pointer type-name='stl-string'/>
        </stl-vector>
        <uint8_t name="tile"/>
    </struct-type>
</data-definition>

