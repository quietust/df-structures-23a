        <uint16_t name='temperature_fraction'/>

        <int16_t name='wear'/>
        <int32_t name='wear_timer' comment='counts up to 806400'/>

        <int32_t name='unk3' init-value='-1'/>
        <int32_t init-value='-1' comment='v0.34.01'/>
    </class-type>

