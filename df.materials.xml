<data-definition>
    <enum-type type-name='material_type'>
        <enum-item name='WOOD'/>
        <enum-item name='STONE_GRAY'/>
        <enum-item name='STONE_LIGHT'/>
        <enum-item name='STONE_DARK'/>
        <enum-item name='GOLD'/>
        <enum-item name='IRON'/>
        <enum-item name='SILVER'/>
        <enum-item name='COPPER'/>
        <enum-item name='GEM_ORNAMENTAL'/>
        <enum-item name='GEM_SEMI'/>
        <enum-item name='GEM_PRECIOUS'/>
        <enum-item name='GEM_RARE'/>
        <enum-item name='BONE'/>
        <enum-item name='IVORY'/>
        <enum-item name='JADE'/>
        <enum-item name='HORN'/>
        <enum-item name='AMBER'/>
        <enum-item name='CORAL'/>
        <enum-item name='PEARL'/>
        <enum-item name='SHELL'/>
        <enum-item name='LEATHER'/>
        <enum-item name='ADAMANTINE'/>
        <enum-item name='SILK'/>
        <enum-item name='PLANT'/>
        <enum-item name='GLASS_GREEN'/>
        <enum-item name='GLASS_CLEAR'/>
        <enum-item name='GLASS_CRYSTAL'/>
        <enum-item name='SAND'/>
        <enum-item name='WATER'/>
        <enum-item name='ZINC'/>
        <enum-item name='TIN'/>
        <enum-item name='COAL'/>
        <enum-item name='BRONZE'/>
        <enum-item name='BRASS'/>
        <enum-item name='STEEL'/>
        <enum-item name='PIGIRON'/>
        <enum-item name='PLATINUM'/>
        <enum-item name='ELECTRUM'/>
        <enum-item name='POTASH'/>
        <enum-item name='ASH'/>
        <enum-item name='PEARLASH'/>
        <enum-item name='LYE'/>
        <enum-item name='RENDERED_FAT'/>
        <enum-item name='SOAP_ANIMAL'/>
        <enum-item name='FAT'/>
        <enum-item name='MUD'/>
        <enum-item name='VOMIT'/>
        <enum-item name='BLOOD'/>
        <enum-item name='SLIME'/>
    </enum-type>

    <enum-type type-name='matgloss_stone_flags'>
        <enum-item name='LIGHT'/>
        <enum-item name='DARK'/>
        <enum-item name='LAVA'/>
        <enum-item name='SHARP'/>
        <enum-item name='RIVER'/>
        <enum-item name='FLUX'/>
    </enum-type>

    <struct-type type-name='matgloss_stone' instance-vector='$global.world.raws.matgloss.stone'>
        <stl-string name='id'/>
        <code-helper name='describe'>$.id</code-helper>

        <int32_t name='flags'/>
        <stl-string name='name'/>
        <uint8_t name='tile'/>
        <static-array type-name='int16_t' name='color' count='3'/>

        <uint16_t name='spec_heat'/>
        <uint16_t name='heatdam_point'/>
        <uint16_t name='colddam_point'/>
        <uint16_t name='ignite_point'/>
        <uint16_t name='melting_point'/>
        <uint16_t name='boiling_point'/>
        <uint16_t name='fixed_temp'/>
    </struct-type>

    <enum-type type-name='matgloss_gem_flags'>
        <enum-item name='ORNAMENTAL'/>
        <enum-item name='SEMI'/>
        <enum-item name='PRECIOUS'/>
        <enum-item name='RARE'/>
        <enum-item name='GLASS'/>
    </enum-type>

    <struct-type type-name='matgloss_gem' instance-vector='$global.world.raws.matgloss.gem'>
        <stl-string name='id'/>
        <code-helper name='describe'>$.id</code-helper>

        <int32_t name='flags'/>
        <static-array type-name='stl-string' name='name' count='2'/>
        <uint8_t name='tile'/>
        <int16_t name='color'/>
        <int32_t name='value'/>

        <uint16_t name='spec_heat'/>
        <uint16_t name='heatdam_point'/>
        <uint16_t name='colddam_point'/>
        <uint16_t name='ignite_point'/>
        <uint16_t name='melting_point'/>
        <uint16_t name='boiling_point'/>
        <uint16_t name='fixed_temp'/>
    </struct-type>

    <enum-type type-name='matgloss_plant_flags'>
        <enum-item name='SWAMP'/>
        <enum-item name='RIVER'/>
        <enum-item name='CAVE'/>
        <enum-item name='FOREST'/>
        <enum-item name='SPRING'/>
        <enum-item name='SUMMER'/>
        <enum-item name='AUTUMN'/>
        <enum-item name='WINTER'/>

        <enum-item/>
        <enum-item name='SEED'/>
        <enum-item name='EDIBLE_RAW'/>
        <enum-item name='EDIBLE_VERMIN'/>
        <enum-item name='LEAVES'/>
        <enum-item name='EXTRACT_VIAL'/>
        <enum-item name='EXTRACT_BAG'/>
        <enum-item name='EXTRACT_BARREL'/>

        <enum-item name='THREAD'/>
        <enum-item name='MILL'/>
        <enum-item name='EXTRACT_STILL_VIAL'/>
        <enum-item name='DRINK'/>
        <enum-item name='EXTRACT_COOKABLE'/>
        <enum-item name='EDIBLE_WHENCOOKED'/>
        <enum-item name='COOKABLE_WHEN_MILLED'/>
        <enum-item name='MILL_DYE'/>
    </enum-type>

    <struct-type type-name='matgloss_plant' instance-vector='$global.world.raws.matgloss.plant'>
        <stl-string name='id'/>
        <code-helper name='describe'>$.id</code-helper>

        <int32_t name='flags'/>
        <stl-string name='name'/>
        <stl-string name='name_plural'/>
        <stl-string name='drink_name'/>
        <stl-string name='seedname'/>
        <stl-string name='mill_name'/>
        <stl-string name='extract_name'/>

        <static-array count='7'> // plant, CLOTH, EXTRACT, DRINK, CHEESE
            <uint16_t name='spec_heat'/>
            <uint16_t name='heatdam_point'/>
            <uint16_t name='colddam_point'/>
            <uint16_t name='ignite_point'/>
            <uint16_t name='melting_point'/>
            <uint16_t name='boiling_point'/>
            <uint16_t name='fixed_temp'/>
        </static-array>

        <stl-string name='mill_dye'/>
        <int16_t name='mill_dye_color' ref-target='descriptor_color'/>
        <uint8_t name='tile'/>
        <uint8_t name='leaves'/>
        <int32_t name='growdur'/>
        <int32_t name='value'/>
        <int32_t name='drinkvalue'/>
        <int32_t name='extractvalue'/>
        <int32_t name='mill_value'/>
        <static-array type-name='int16_t' name='color' count='3'/>
        <static-array type-name='int16_t' name='seed_color' count='3'/>
        <static-array type-name='int16_t' name='drink_color' count='3'/>
        <static-array type-name='int16_t' name='extract_color' count='3'/>
        <static-array type-name='int16_t' name='mill_color' count='3'/>
        <int16_t name='genpower'/>
        <int16_t name='frequency'/>
        <int16_t name='clustersize'/>

        <stl-vector name='prefstring'>
            <pointer type-name='stl-string'/>
        </stl-vector>
    </struct-type>

    <enum-type type-name='matgloss_wood_flags'>
        <enum-item name='AUTUMNCOLOR'/>
        <enum-item name='WET'/>
        <enum-item name='DRY'/>
        <enum-item name='BIOME_MOUNTAIN'/>
        <enum-item name='BIOME_GLACIER'/>
        <enum-item name='BIOME_TUNDRA'/>
        <enum-item name='BIOME_SWAMP_TEMPERATE_FRESHWATER'/>
        <enum-item name='BIOME_SWAMP_TEMPERATE_SALTWATER'/>

        <enum-item name='BIOME_MARSH_TEMPERATE_FRESHWATER'/>
        <enum-item name='BIOME_MARSH_TEMPERATE_SALTWATER'/>
        <enum-item name='BIOME_SWAMP_TROPICAL_FRESHWATER'/>
        <enum-item name='BIOME_SWAMP_TROPICAL_SALTWATER'/>
        <enum-item name='BIOME_SWAMP_MANGROVE'/>
        <enum-item name='BIOME_MARSH_TROPICAL_FRESHWATER'/>
        <enum-item name='BIOME_MARSH_TROPICAL_SALTWATER'/>
        <enum-item name='BIOME_FOREST_TAIGA'/>

        <enum-item name='BIOME_FOREST_TEMPERATE_CONIFER'/>
        <enum-item name='BIOME_FOREST_TEMPERATE_BROADLEAF'/>
        <enum-item name='BIOME_FOREST_TROPICAL_CONIFER'/>
        <enum-item name='BIOME_FOREST_TROPICAL_DRY_BROADLEAF'/>
        <enum-item name='BIOME_FOREST_TROPICAL_MOIST_BROADLEAF'/>
        <enum-item name='BIOME_GRASSLAND_TEMPERATE'/>
        <enum-item name='BIOME_SAVANNA_TEMPERATE'/>
        <enum-item name='BIOME_SHRUBLAND_TEMPERATE'/>

        <enum-item name='BIOME_GRASSLAND_TROPICAL'/>
        <enum-item name='BIOME_SAVANNA_TROPICAL'/>
        <enum-item name='BIOME_SHRUBLAND_TROPICAL'/>
        <enum-item name='BIOME_DESERT_BADLAND'/>
        <enum-item name='BIOME_DESERT_ROCK'/>
        <enum-item name='BIOME_DESERT_SAND'/>
        <enum-item name='BIOME_OCEAN'/>
        <enum-item name='BIOME_INLAND_FRESHWATER'/>

        <enum-item name='BIOME_INLAND_BRACKISHWATER'/>
        <enum-item name='BIOME_INLAND_SALTWATER'/>
        <enum-item name='BIOME_SUBTERRANEAN_RIVER'/>
        <enum-item name='BIOME_SUBTERRANEAN_CHASM'/>
        <enum-item name='BIOME_SUBTERRANEAN_LAVA'/>
    </enum-type>

    <struct-type type-name='matgloss_wood' instance-vector='$global.world.raws.matgloss.wood'>
        <stl-string name='id'/>
        <code-helper name='describe'>$.id</code-helper>
        <df-flagarray name='flags' index-enum='matgloss_wood_flags'/>
        <stl-string name='name'/>
        <stl-string name='adj'/>
        <stl-vector name='prefstring'>
            <pointer type-name='stl-string'/>
        </stl-vector>

        <uint16_t name='spec_heat'/>
        <uint16_t name='heatdam_point'/>
        <uint16_t name='colddam_point'/>
        <uint16_t name='ignite_point'/>
        <uint16_t name='melting_point'/>
        <uint16_t name='boiling_point'/>
        <uint16_t name='fixed_temp'/>
    </struct-type>

    <struct-type type-name='material_vec_ref'>
        <stl-vector name='material'>
            <enum base-type='int16_t' type-name='material_type'/>
        </stl-vector>
        <stl-vector name='matgloss'>
            <int16_t refers-to='(matgloss-target $$._parent._parent.material[$$._key] $)'/>
        </stl-vector>
    </struct-type>

    <struct-type type-name='contaminant'>
        <int32_t name='state'/> 0/1 = solid/liquid?
        <enum base-type='int32_t' name='material' type-name='material_type'/>
        <int32_t name='amount'/>
    </struct-type>
</data-definition>

